pipeline {
	agent any
	stages {
		stage('Checkout') {
			steps {
				echo 'Running Checkout stage'
			}
			when  {
	 branch 'main'
	}
		}

		stage('Test') {
			steps {
				echo 'Running Test stage'
			}
		}

		stage('Build') {
			steps {
				echo 'Running Build stage'
			}
		}

		stage('Deploy') {
			steps {
				echo 'Running Deploy stage'
			}
		}
}
}
