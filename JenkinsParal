pipeline {
	agent none 
	stages {
		stage('Stage-1') {
      			agent {label 'master'}
			steps {
				sh 'touch master'
       				 sh 'mkdir master-1'
			}
		}
		stage('Deploy') {
    			  agent {label 'java_node'}
			steps {
				sh '''touch java_node
             			 mkdir java_node_1'''
			}
		}
	}
}
